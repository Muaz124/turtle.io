{
	"auth"    : {},
	"compress": true,
	"default" : null,
	"uid"     : null,
	"cache"   : 1000,
	"headers" : {
		"Access-Control-Allow-Origin" : "*",
		"Access-Control-Allow-Headers": "*",
		"Access-Control-Allow-Methods": "",
		"Allow"                       : "",
		"Cache-Control"               : "max-age=60, must-revalidate, public",
		"Content-Type"                : "text/html; charset=utf-8",
		"Date"                        : "",
		"Last-Modified"               : ""
	},
	"index"   : ["index.htm", "index.html"],
	"lag"     : 50,
	"logs"    : {
		"time"   : "D/MMM/YYYY:HH:mm:ss ZZ",
		"format" : "%v %h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-agent}i\"",
		"level"  : "info",
		"stdout" : true,
		"syslog" : true
	},
	"port"    : 8000,
	"root"    : "",
	"session" : {
		"id"    : "sid",
		"valid" : "15m",
		"gc"    : 60000
	},
	"pages"   : null,
	"proxy"   : {
		"rewrite" : [
			"text/html",
			"text/plain",
			"application/json"
		]
	},
	"ssl"     : {
		"key"  : null,
		"cert" : null
	},
	"tmp"     : "/tmp",
	"vhosts"  : {}
}