{
	"auth"    : {},
	"compress": true,
	"default" : null,
	"uid"     : null,
	"cache"   : 1000,
	"headers" : {
		"access-control-allow-origin"   : "*",
		"access-control-allow-headers"  : "allow, authorization, cache-control, content-length, content-type, etag, expires, last-modified",
		"access-control-allow-methods"  : "",
		"access-control-expose-headers" : "allow, content-length",
		"accept-ranges"                 : "bytes",
		"allow"                         : "",
		"cache-control"                 : "max-age=60, must-revalidate, public",
		"content-type"                  : "text/html; charset=utf-8",
		"date"                          : "",
		"last-modified"                 : "",
		"x-content-type-options"        : "nosniff",
		"x-frame-options"               : "sameorigin",
		"x-ua-compatible"               : "IE=edge"
	},
	"index"   : ["index.htm", "index.html"],
	"logs"    : {
		"time"   : "D/MMM/YYYY:HH:mm:ss ZZ",
		"format" : "%v %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-agent}i\"",
		"level"  : "info",
		"stdout" : true,
		"syslog" : true,
		"dtrace" : true
	},
	"port"    : 8000,
	"root"    : "",
	"pages"   : null,
	"proxy"   : {
		"rewrite" : [
			"text/html",
			"text/plain",
			"application/json"
		],
		"maxConnections" : 1000
	},
	"ssl"     : {
		"key"  : null,
		"cert" : null
	},
	"json"    : 2,
	"tmp"     : "/tmp",
	"vhosts"  : {}
}