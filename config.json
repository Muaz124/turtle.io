{
	"auth"    : {},
	"compress": true,
	"default" : null,
	"uid"     : null,
	"cache"   : 1000,
	"headers" : {
		"access-control-allow-origin" : "*",
		"access-control-allow-headers": "*",
		"access-control-allow-methods": "",
		"accept-ranges"               : "bytes",
		"allow"                       : "",
		"cache-control"               : "max-age=60, must-revalidate, public",
		"content-type"                : "text/html; charset=utf-8",
		"date"                        : "",
		"last-modified"               : ""
	},
	"index"   : ["index.htm", "index.html"],
	"lag"     : 50,
	"logs"    : {
		"time"   : "D/MMM/YYYY:HH:mm:ss ZZ",
		"format" : "%v %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-agent}i\"",
		"level"  : "info",
		"stdout" : true,
		"syslog" : true
	},
	"port"    : 8000,
	"root"    : "",
	"session" : {
		"id"    : "sid",
		"valid" : "15m",
		"gc"    : 60000
	},
	"pages"   : null,
	"proxy"   : {
		"rewrite" : [
			"text/html",
			"text/plain",
			"application/json"
		],
		"maxConnections" : 1000
	},
	"ssl"     : {
		"key"  : null,
		"cert" : null
	},
	"tmp"     : "/tmp",
	"vhosts"  : {}
}